<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="false" android:state_checked="true">
		<shape>
			<solid android:color="@color/color_5ecb6c"/>
			<corners android:radius="99dp"/>
		</shape>
	</item>
	<item android:state_enabled="false" android:state_checked="false">
		<shape>
			<solid android:color="@color/color_cfd1d8"/>
			<corners android:radius="99dp"/>
			<stroke
				android:width="1dp"
				android:color="@color/color_cfd1d8"/>
		</shape>
	</item>
	<item android:state_checked="true">
		<shape>
			<solid android:color="@color/color_cfd1d8"/>
			<corners android:radius="99dp"/>
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/color_5ecb6c"/>
			<corners android:radius="99dp"/>
			<stroke
				android:width="1dp"
				android:color="@color/color_5ecb6c"/>
		</shape>
	</item>
</selector>